{
  "name": "the-lot",
  "version": "0.0.1",
  "type": "commonjs",
  "private": true,
  "scripts": {
    "ng": "ng",
    "services:up": "docker compose -f src/main/docker/services.yml up --wait",
    "start": "ng serve --hmr",
    "start-tls": "npm run webapp:dev-ssl",
    "build": "npm run webapp:prod --",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "stubdata": "json-server data.js -p 3500",
    "clean-www": "rimraf build/resources/main/static/",
    "cleanup": "rimraf build/",
    "webapp:build": "npm run clean-www && npm run webapp:build:dev",
    "webapp:build:dev": "ng build --configuration development",
    "webapp:build:prod": "ng build --configuration production",
    "webapp:dev": "ng serve",
    "webapp:dev-ssl": "ng serve --ssl",
    "webapp:dev-verbose": "ng serve --verbose",
    "webapp:prod": "npm run clean-www && npm run webapp:build:prod",
    "webapp:test": "npm run test --",
    "jest": "jest --coverage --logHeapUsage --maxWorkers=2 --config jest.conf.js"
  },
  "config": {
    "backend_port": 8080,
    "default_environment": "prod",
    "packaging": "jar"
  },
  "dependencies": {
    "@angular/animations": "^18.2.0",
    "@angular/common": "^18.2.0",
    "@angular/compiler": "^18.2.0",
    "@angular/core": "^18.2.9",
    "@angular/forms": "^18.2.0",
    "@angular/platform-browser": "^18.2.0",
    "@angular/platform-browser-dynamic": "^18.2.0",
    "@angular/platform-server": "^18.2.0",
    "@angular/router": "^18.2.0",
    "@fortawesome/angular-fontawesome": "0.15.0",
    "@fortawesome/fontawesome-svg-core": "6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@ng-bootstrap/ng-bootstrap": "17.0.1",
    "@popperjs/core": "2.11.8",
    "bootstrap": "^5.3.3",
    "chart.js": "^4.4.6",
    "json-server": "^1.0.0-beta.3",
    "ng2-charts": "^6.0.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "18.0.0",
    "@angular-builders/jest": "18.0.0",
    "@angular-devkit/build-angular": "^18.2.4",
    "@angular/cli": "^18.2.4",
    "@angular/compiler-cli": "^18.2.0",
    "@angular/service-worker": "^18.2.0",
    "@types/jest": "29.5.13",
    "@types/node": "^18.18.0",
    "browser-sync": "3.0.2",
    "browser-sync-webpack-plugin": "2.3.0",
    "buffer": "6.0.3",
    "copy-webpack-plugin": "12.0.2",
    "globals": "15.9.0",
    "jasmine-core": "~5.2.0",
    "jest": "29.7.0",
    "jest-date-mock": "1.0.10",
    "jest-environment-jsdom": "29.7.0",
    "jest-junit": "16.0.0",
    "jest-preset-angular": "14.2.4",
    "jest-sonar": "0.2.16",
    "rimraf": "5.0.8",
    "ts-jest": "29.2.5",
    "typescript": "~5.5.2",
    "wait-on": "8.0.1",
    "webpack-bundle-analyzer": "4.10.2",
    "webpack-merge": "6.0.1",
    "webpack-notifier": "1.15.0"
  },
  "engines": {
    "node": ">=20.17.0"
  },
  "cacheDirectories": [
    "node_modules"
  ],
  "overrides": {
    "browser-sync": "3.0.2",
    "webpack": "5.94.0"
  }
}
